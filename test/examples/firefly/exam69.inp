 $CONTRL 
   SCFTYP=MCSCF RUNTYP=OPTIMIZE NZVAR=3 COORD=ZMT INTTYP=HONDO ICUT=12
 $END
 $SYSTEM TIMLIM=15 MEMORY=300000 $END
 $BASIS  GBASIS=N31 NGAUSS=6 ndfunc=1 $END
 $GUESS  GUESS=MOREAD NORB=19 $END
!
! Note, high computation accuracy is enforced throughout 
! 
 $trans dirtrf=.t. mptran=2 mode=0 cuttrf=1.0d-13 $end
 $gugem pack2=1 cutoff=1d-12 $end
 $gugdm2 wstate(1)=1,1 cutoff=1d-12 $end 
 $gugdia nstate=2 itermx=1000 cvgtol=1d-7 $end
 $DRT  GROUP=C2V ISTSYM=1 FORS=.TRUE. NMCC=3 NDOC=1 NVAL=1 $END
 $MCSCF cistep=guga istate=1 acurcy=1.0d-7 npflg(9)=1 $END
 $ZMAT IZMAT(1)=1,1,2,   1,1,3,   2,2,1,3  $END
 $DATA
Methylene...1-A-1 state...MCSCF/6-31G*
Cnv  2

C
H 1 rCH
H 1 rCH 2 aHOH

rCH=1.09
aHOH=99.0
 $END
